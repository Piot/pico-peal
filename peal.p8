pico-8 cartridge // http://www.pico-8.com
version 5
__lua__
avatar = {}
avatar.x = 8
avatar.y = 8
avatar.dirx = 0
avatar.diry = 0
avatar.ate_mega_pill_time = 0

input = {}
input.dirx = 0
input.diry = 0

SPRITE_PILL = 2
SPRITE_MEGA_PILL = 5
SPRITE_NOTHING = 0

SPRITE_AVATAR_ANIM_1 = 4
SPRITE_AVATAR_ANIM_2 = 1

SFX_MOVE = 1
SFX_EAT = 2
SFX_EAT_MEGA = 3

t = 0

function can_change_dir(avatar)
	return (avatar.x % 8 == 0) and (avatar.y % 8 == 0)
end

function is_moving(avatar)
	return avatar.dirx != 0 or avatar.diry != 0
end

function tilepos(pixel)
	local tilepos = {}
	tilepos.x = (pixel.x) / 8
	tilepos.y = (pixel.y) / 8
	return tilepos
end

function tile(tilepos)
	return mget(tilepos.x, tilepos.y)
end

function collides(pos)
	local tile = tile(pos)
	return tile != SPRITE_NOTHING and tile != SPRITE_MEGA_PILL and tile != SPRITE_PILL
end

function can_eat(pos)
	local tile = tile(pos)
	return tile == SPRITE_PILL and 1 or (tile == SPRITE_MEGA_PILL and 2)
end

function _update()
	t += 1
	avatar.x += avatar.dirx
	avatar.y += avatar.diry

	local x, y = avatar_input()

	local has_input = x != 0 or y != 0
	if has_input then
		input.dirx = x
		input.diry = y
	end

	if can_change_dir(avatar) then
		local avatar_pos = tilepos(avatar)
		local has_next_input = input.dirx != 0 or input.diry != 0
		if has_next_input then
			local input_pos = {}
			input_pos.x = avatar_pos.x + input.dirx
			input_pos.y = avatar_pos.y + input.diry
			if not collides(input_pos) then
				avatar.dirx = input.dirx
				avatar.diry = input.diry
				input.diry = 0
				input.dirx = 0
			end
		end

		local new_position = {}
		new_position.x = avatar_pos.x + avatar.dirx
		new_position.y = avatar_pos.y + avatar.diry
		local can_move = not collides(new_position)
		local can_eat = can_eat(new_position)
		if can_eat then
			mset(new_position.x, new_position.y, 0)
			if can_eat == 2 then
				avatar.ate_mega_pill_time = t
			end
		end
		avatar.ate_pill = can_eat
		if can_change_dir(avatar) and not can_move then
			avatar.dirx = 0
			avatar.diry = 0
		end
	else
		avatar.ate_pill = false
	end
end

function _draw()
	cls()
	draw_map()
	draw_avatar(avatar)
	avatar_sound(avatar)
end

function draw_map()
	map(0,0,0,0,20,20)
end

function draw_avatar(avatar)
	local avatar_sprite_id = (flr(t / 10) % 2 == 0) and SPRITE_AVATAR_ANIM_1 or SPRITE_AVATAR_ANIM_2
	spr(avatar_sprite_id, avatar.x, avatar.y)
end

function avatar_input()
	local x, y
	x = btn(0) and -1 or (btn(1) and 1 or 0)
	if x == 0 then
		y = btn(2) and -1 or (btn(3) and 1 or 0)
	else
		y = 0
	end
	return x, y
end

function avatar_sound(avatar)
	if is_moving(avatar) and ((avatar.x + avatar.y) % 16) == 0 then
		sfx(SFX_MOVE)
	end

	if avatar.ate_pill then
		local sound = avatar.ate_pill == 2 and SFX_EAT_MEGA or SFX_EAT
		local channel = avatar.ate_pill
		sfx(sound, channel)
	end
end

